{
  "name": "ffmpeg-template-assembly",
  "version": "0.1.0",
  "module": "./dist/index.js",
  "types": "./dist/core/types.d.ts",
  "author": "heristop",
  "license": "MIT",
  "scripts": {
    "compile": "ts-node-esm src/main.ts",
    "format": "prettier --write .",
    "lint": "eslint src tests --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/shared/assets/*",
      "!src/shared/assets/**/*"
    ],
    "format": [
      "esm"
    ],
    "splitting": false,
    "sourcemap": false,
    "dts": true,
    "clean": true
  },
  "type": "module",
  "dependencies": {
    "axios": "^1.6.2",
    "extract-zip": "^2.0.1",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.2.0",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "pino": "^8.17.1",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.101",
    "@types/ffmpeg": "^1.0.7",
    "@types/ffprobe": "^1.1.8",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-istanbul": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  }
}
